<h1>Bookings</h1>

  <table>
    <tr>
      <th>Members</th>
      <th></th>
      <th>Classes</th>
      <th></th>
      <th>Date</th>
      <th></th>
      <th>Time</th>

    </tr>
    <% for booking in @bookings %>
    <tr>
      <td><%= booking.member.first_name %> <%= booking.member.last_name %></td>
      <td>|</td>
      <td><%= booking.gym_class.title %></td>
      <td>|</td>
      <td><%= booking.gym_class.date_class %></td>
      <td>|</td>
      <td><%= booking.gym_class.time %></td>
    </tr>
    <% end %>
  </table>
<br>
<a href="/bookings/new_member"><input class="btn" type="submit" value="Create a New Booking"/>
